{
  "name": "gthor",
  "version": "1.4.5",
  "description": "Expose GraphQL queries, mutations and subscription as configurable RESTful endpoints",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges output \"badges\"",
    "format": "prettier --write **/*.{js,md,json}",
    "docs": "documentation build src/** -f md -o docs/DOCS.md"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-f-r/gthor.git"
  },
  "keywords": [
    "graphql",
    "rest",
    "gthor"
  ],
  "author": "Joao Furtado Resende",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/j-f-r/gthor/issues"
  },
  "homepage": "https://github.com/j-f-r/gthor#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "jest-coverage-badges": "^1.1.2"
  }
}
